   "source": [
    "word = input().lower()\n",
    "word_list = list(set(word))\n",
    "\n",
    "cnt =[]\n",
    "for i in word_list :\n",
    "    count = word.count(i)\n",
    "    cnt.append(count)\n",
    "    \n",
    "if cnt.count(max(cnt)) >=2 : #가장 큰 최대값이 2개 이상일 경우, ? 출력\n",
    "    print(\"?\")\n",
    "    \n",
    "else : \n",
    "    print(word_list[(cnt.index(max(cnt)))].upper())"
   ]
